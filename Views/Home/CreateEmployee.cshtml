                                                <!--  <button type="button" data-toggle="modal" data-target="#ResumeModal"  class="btn btn-success">Create a New Entry</button>-->
                                                @foreach (var item in Model.EmployeeResumesTypes)
                                                {
                                                    <div class="card border-0" id="@item.Id">
                                                        <div class="card-header">
                                                            <b> @item.Name  </b>
                                                            <button type="button" data-toggle="modal" data-target="#ResumeModal" class="btn btn-secondary  float-right btn-sm ">Add</button>
                                                        </div>
                                                        <div class="card-body" id="body_@item.Id">
                                                            @foreach (var child in Model.EmployeeResumes)
                                                            {
                                                                if (item.Id == child.LineTypeId.ToString())
                                                                {
                                                                    <div id="@child.Id">
                                                                        <p class="d-inline">@child.DateStart?.ToString("yyyy-MM-dd")</p><p class="d-inline">-@child.DateEnd?.ToString("yyyy-MM-dd")</p>
                                                                        <h6 class="card-text">@child.Name</h6>
                                                                        <p>@child.Description</p>
                                                                    </div>
                                                                }

                                                            }

                                                        </div>
                                                    </div>

                                                }

                                            </div>
                                            <div class="col-sm-6">
