                        DateStart: startdate,
                        DateEnd: Enddate,
                        lineLineTypeId: typeid,
                    };
        resumes.push(o)
        console.log(resumes);
        $('#sam')[0].reset();


    }

        function EditFile(event) {
            switch (event.id) {
            case 'choose-file1-a':
                $('#choose-file1').trigger('click');
                    break;
                case 'choose-file2-a':
                    $('#choose-file2').trigger('click');
                    break;
                case 'choose-file3-a':
                    $('#choose-file3').trigger('click');
                    break;
                case 'choose-file4-a':
                    $('#choose-file4').trigger('click');
                    break;
            }


        }
        function EditEmployeeImage() {
            $('#imgw').trigger('click');

        }
        function ChangeFile(event) {

            switch (event.target.id) {
                case 'choose-file1':
                    $('#choose-file1-label').val($('#choose-file1').val());
                    break;
                case 'choose-file2':
                    $('#choose-file2-label').val($('#choose-file2').val());
                    break;
                case 'choose-file3':
                    $('#choose-file3-label').val($('#choose-file3').val());
                    break;
                case 'choose-file4':
                    $('#choose-file4-label').val($('#choose-file4').val());
                    break;
            }
        }

        function removeFile(event) {
            switch (event.id) {
                case 'choose-file1-a1':

                    $('#choose-file1-label').val(null);
                    $('#choose-file1').val(null);
                    $('#choose-file1div').hide();
                    $('#choosefileDiv1').attr("hidden", false);
                    break;
                case 'choose-file2-a1':

                    $('#choose-file2-label').val(null);
                    $('#choose-file2').val(null);
                    $('#choose-file2div').hide();
                    $('#choosefileDiv2').attr("hidden", false);
                    break;
                case 'choose-file3-a1':

                    $('#choose-file3-label').val(null);
                    $('#choose-file3').val(null);
                    $('#choose-file3div').hide();
                    $('#choosefileDiv3').attr("hidden", false);
                    break;
                case 'choose-file4-a1':

                    $('#choose-file4-label').val(null);
                    $('#choose-file4').val(null);
                    $('#choose-file4div').hide();
                    $('#choosefileDiv4').attr("hidden", false);
                    break;
            }

        }
        function selectchange() {
            var selected = $('#mar option:selected').text();
            if (selected == "married") {
                $("#hide").attr("hidden", false)
            }
            else { $("#hide").attr("hidden", true) }
        }

        function selectchangejob() {
            $("#job").val($('#jobselect option:selected').text())

        }
        function DeleteImage() {

            $('#encode').val("");
            $('#imgw').val("");
            $('#blah').attr("src", "../../images/PICA.jpg");

        }

        var dependentCount = 0;

        function Delete(id) {
            $('#' + id).remove();
            arr = arr.filter(item => item !== id);
            dependentCount--;
            $('#countdependant').text(dependentCount)
        }
        function create() {
            count++;
            $('#mcontent').append(
                '<tr id="' + count + '">' +
                '<td><input id="name_' + count + '" type="text" placeholder="name" /></td>' +
                '<td><input id="birth_' + count + '" type="date" /></td>' +
                '<td><a href="javascript:;" onclick="Delete(' + count + ')"><i class="fa fa-trash"></i></a></td>' +
                '<td><input hidden type="date" /></td>' +
                '</tr>');
            arr.push(count);
            dependentCount++;
            $('#countdependant').text(dependentCount)
        }
        var obj = new Array();
        function SubmitForm() {


            obj = [];
            for (let index = 0; index < arr.length; index++) {
                obj.push({
                    Name: $('#name_' + arr[index]).val(),
                    Bdate: $('#birth_' + arr[index]).val(),
                    Id: parseInt($('#id_' + arr[index]).val())
                });
            }
            console.log(obj);
            $('#dependant').val(JSON.stringify(obj));
            $('#DataForm').submit();

        }
</script>
