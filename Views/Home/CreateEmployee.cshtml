                function readURL(input) {
                    if (input.files && input.files[0]) {
                        var reader = new FileReader();

                        reader.onload = function (e) {
                        $('#blah').attr('src', e.target.result);
                        $('#encode').attr('value', e.target.result);
                        }

                        reader.readAsDataURL(input.files[0]); // convert to base64 string
                    }
                }

            $("#imgw").change(function () {
                    readURL(this);
                });



        });



    function saveResume() {
        var typeid = $('#ResumeSelectType').val();
        var startdate = $('#ResumeStartDate').val();
        var Enddate = $('#ResumeEnddate').val();
        var discription = $('#ResumeDescription').val();
        var name = $('#ResumeName').val();
        var html = [
            '<div id="c_' + childcount + '">',
    '<p class="d-inline">' + startdate + '</p><p class="d-inline">-' + Enddate + '</p>',
    ' <h6 class="card-text">' + name + '</h6>',
    ' <p>' + discription + '</p>',
       '</div>'].join('');
        $('#body_' + typeid).append(html);
          var o = {
